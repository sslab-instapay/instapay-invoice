  "bugs": {
    "url": "https://github.com/bitcoinjs/bolt11/issues"
  },
  "homepage": "https://github.com/bitcoinjs/bolt11#readme",
  "dependencies": {
    "@types/bn.js": "^4.11.3",
    "bech32": "^1.1.2",
    "bitcoinjs-lib": "^3.3.1",
    "bn.js": "^4.11.8",
    "coininfo": "^4.5.0",
    "lodash": "^4.17.19",
    "safe-buffer": "^5.1.1",
    "secp256k1": "^3.4.0"
